{
  "compilerOptions": {
    "paths": {
      "@/*": ["./*"]
    },
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx",
    "app/api/auth/[...nextauth]/route.js"
, "components/application/app-sidebar.js", "components/application/nav-main.js", "components/application/nav-projects.js", "components/application/nav-secondary.js", "components/application/nav-user.js"  ],
  "exclude": ["node_modules"]
}
