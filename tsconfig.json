{
  "compilerOptions": {
    "paths": {
      "@/*": ["./*"]
    },
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx",
    "app/api/auth/[...nextauth]/route.js"
, "components/application/app-sidebar.js", "components/application/nav-main.js", "components/application/nav-projects.js", "components/application/nav-secondary.js", "components/application/nav-user.js", "app/(application)/(user)/alert/page.jsx", "app/api/scrape/route.js", "app/api/product/route.js", "app/api/list/route.js", "app/api/list/[id]/route.js", "app/api/schedule/route.js", "app/api/schedule/[id]/route.js", "app/api/product/[id]/route.js", "test/sample.test.js", "script.js"  ],
  "exclude": ["node_modules"]
}
